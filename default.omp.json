{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "final_space": true,
    "console_title": true,
    "console_title_style": "",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "horizontal_offset": 0,
            "vertical_offset": 0,
            "segments": [
                {
                    "type": "os",
                    "style": "diamond",
                    "leading_diamond": "\ue0b6",
                    "trailing_diamond": "\ue0b0",
                    "foreground": "#f2f7f7",
                    "background": "#546E7A",
                    "properties": {
                        "postfix": "  ",
                        "prefix": "  ",
                        "macos": "mac",
                        "wsl": "üêß",
                        "wsl_separator": " "
                    }
                },
                {
                    "type": "path",
                    "style": "powerline",
                    "powerline_symbol": "\ue0b0",
                    "invert_powerline": false,
                    "foreground": "#ffffff",
                    "background": "#ff479c",
                    "properties": {
                        "prefix": " üìÅ ",
                        "style": "folder"
                    }
                },
                {
                    "type": "git",
                    "style": "powerline",
                    "powerline_symbol": "\ue0b0",
                    "invert_powerline": false,
                    "foreground": "#193549",
                    "background": "#fffb38",
                    "properties": {
                        "display_stash_count": true,
                        "display_upstream_icon": true,
                        "local_working_icon": " üìù",
                        "local_staged_icon": " ‚úÖ",
                        "stash_count_icon": "üíæ "
                    }
                },
                {
                    "type": "node",
                    "style": "powerline",
                    "powerline_symbol": "\ue0b0",
                    "invert_powerline": false,
                    "foreground": "#ffffff",
                    "background": "#6CA35E",
                    "properties": {
                        "prefix": " Óúò "
                    }
                },
                {
                    "type": "dotnet",
                    "style": "powerline",
                    "display_mode": "always",
                    "powerline_symbol": "\ue0b0",
                    "foreground": "#000000",
                    "background": "#00ffff",
                    "properties": {
                        "prefix": " \uE77F "
                    }
                },
                {
                    "type": "angular",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#000000",
                    "background": "#d21919",
                    "properties": {
                        "prefix": " \uE753 "
                    }
                },
                {
                    "type": "java",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#1f1f1f",
                    "background": "#87e7e7",
                    "properties": {
                        "prefix": " \uE738 "
                    }
                },
                {
                    "type": "shell",
                    "style": "powerline",
                    "powerline_symbol": "\ue0b0",
                    "invert_powerline": false,
                    "foreground": "#ffffff",
                    "background": "#0077c2",
                    "properties": {
                        "prefix": " \uf120 "
                    }
                },
                {
                    "type": "root",
                    "style": "powerline",
                    "powerline_symbol": "ÓÇ∞",
                    "invert_powerline": false,
                    "foreground": "#193549",
                    "background": "#ffff66"
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "right",
            "segments": [
                {
                    "type": "sysinfo",
                    "style": "powerline",
                    "powerline_symbol": "\ue0b2",
                    "invert_powerline": true,
                    "foreground": "#ffffff",
                    "background": "#8f43f3",
                    "properties": {
                        "prefix": " \uf85a ",
                        "postfix": "% ",
                        "precision": 2,
                        "template": "{{ (round .PhysicalPercentUsed .Precision) }}"
                    }
                },
                {
                    "type": "time",
                    "style": "powerline",
                    "powerline_symbol": "\ue0b2",
                    "invert_powerline": true,
                    "foreground": "#007ACC",
                    "background": "#dbdbdb",
                    "properties": {
                        "time_format": "15:04",
                        "postfix": " "
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "newline": true,
            "alignment": "left",
            "horizontal_offset": 0,
            "vertical_offset": 0,
            "segments": [
                {
                    "type": "exit",
                    "style": "diamond",
                    "foreground": "#ffffff",
                    "background": "#2e9599",
                    "trailing_diamond": "\ue0b0",
                    "leading_diamond": "\ue0b6",
                    "properties": {
                        "always_enabled": true,
                        "color_background": true,
                        "display_exit_code": false,
                        "error_color": "#f1184c",
                        "prefix": "üßë‚Äçüíª",
                        "postfix": " "
                    }
                }
            ]
        }
    ]
}