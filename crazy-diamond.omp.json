{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{if .Root}}(Admin) {{end}}{{.Folder}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "type": "os",
          "style": "diamond",
          "background": "lightBlue",
          "foreground": "black",
          "leading_diamond": "\u256d\u2500\ue0b6"
        },
        {
          "type": "root",
          "style": "diamond",
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b0",
          "template": " \uf0e7 ",
          "foreground": "foreground",
          "background": "red"
        },
        {
          "type": "path",
          "style": "diamond",
          "properties": {
            "cache_duration": "none",
            "folder_icon": "...",
            "folder_separator_icon": "<transparent> \ue0bd </>",
            "home_icon": "\ueb06",
            "style": "folder"
          },
          "leading_diamond": " ",
          "trailing_diamond": "\ue0b0",
          "template": " \ue5ff {{ .Path }} ",
          "foreground": "foreground",
          "background": "blue"
        },
        {
          "type": "git",
          "style": "diamond",
          "template": " {{ .UpstreamIcon }} {{ .HEAD }} ({{ printf \"%.7s\" .Commit.Sha}}){{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf51e {{ .StashCount }}{{ end }} ",
          "foreground": "black",
          "background": "yellow",
          "properties": {
            "branch_max_length": 25,
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "source": "pwsh",
            "branch_icon": "\ue725"
          }
        },
        {
          "type": "java",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "white",
          "background": "red",
          "template": " \ue256 {{ .Full }} ",
          "properties": {
            "fetch_version": true
          }
        },
        {
          "type": "aws",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "leading_diamond": "",
          "foreground": "lightRed",
          "background": "darkGray",
          "template": " \uf0ef  {{.Profile}}{{if .Region}}@{{.Region}}{{end}} "
        },
        {
          "properties": {
            "always_enabled": false,
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b0",
          "template": " \ue654 ",
          "foreground": "foreground",
          "background": "yellow",
          "type": "status",
          "style": "diamond",
          "background_templates": [
            "{{ if .Error }}red{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "austin",
            "threshold": 500
          },
          "leading_diamond": "\ue0c5",
          "trailing_diamond": "\ue0bc",
          "template": "  <foreground>\uf254</> {{ .FormattedMs }} ",
          "foreground": "foreground",
          "background": "magenta",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "foreground": "magenta",
          "background": "transparent",
          "style": "diamond",
          "template": " <lightWhite>\uebe2 </> {{ round .PhysicalPercentUsed .Precision }}%",
          "type": "sysinfo"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": false,
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "<transparent,blue>\ue0b0</>",
          "trailing_diamond": "\ue0b0",
          "template": " \ue70c {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "white",
          "background": "blue",
          "type": "dotnet",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if .Env.CONDA_DEFAULT_ENV }}\ue235  ({{ .Env.CONDA_DEFAULT_ENV }}){{ end }} ",
          "foreground": "white",
          "type": "text",
          "style": "plain"
        },
        {
          "type": "kubectl",
          "style": "diamond",
          "leading_diamond": "<lightBlue>\u2570\u2500{ </>",
          "trailing_diamond": "<lightBlue> }</>",
          "foreground": "white",
          "background": "transparent",
          "template": "<lightBlue>\udb82\udc33</> {{if .Namespace}}{{.Namespace}} <lightBlue>at</> {{end}}{{.Context}}"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf4b5  ",
          "foreground": "lightGreen",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}